<div class="container">
	<div class="row">
		<div class="col-md-12">

			<h1>Listing home_categories</h1>

			<table class="table">
			  <tr>
			    <th>Title</th>
			    <th>See more text</th>
					<th>Keyword</th>
			    <th>Description</th>
			    <th></th>
			    <th></th>
			    <th></th>
			  </tr>

			<% @home_categories.each do |home_category| %>
			  <tr>
			    <td><%= home_category.title %></td>
			    <td><%= home_category.see_more_text %></td>
			    <td><%= home_category.category_keyword_for_url %></td>
			    <td><%= home_category.description %></td>
			    <td><%= link_to 'Show', home_category %></td>
			    <td><%= link_to 'Edit', edit_home_category_path(home_category) %></td>
			    <td><%= link_to 'Destroy', home_category, confirm: 'Are you sure?', method: :delete %></td>
			  </tr>
			<% end %>
			</table>

			<br />

			<%= link_to 'New Home category', new_home_category_path %>
		</div>
	</div>
</div>